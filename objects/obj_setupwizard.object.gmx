            <kind>1</kind>
            <string>init_text = keyboard_string;

if(setup == "name")
{
    if(mouse_check_button_released(mb_left))
    {
        if(point_in_rectangle(mouse_x, mouse_y, 1200, 640, 1246, 686))
        {
            ini_open(game_save_id + '_data.ini');
            ini_write_string("User Data", "profile_name", init_text);
            ini_close();
            setup = 1;
        }
    }
}
else if(setup == "picture")
{
    if(mouse_check_button_released(mb_left))
    {
        if(point_in_rectangle(mouse_x, mouse_y, 300, 256, 428, 280))
        {
            add_profilepicture();
        }
        if(point_in_rectangle(mouse_x, mouse_y, 300, 320, 428, 344))
        {
            profile_icon = spr_default_profilepicture
        }
        if(point_in_rectangle(mouse_x, mouse_y, 1200, 640, 1246, 686))
        {
            setup = 2;
        }
    }
}
