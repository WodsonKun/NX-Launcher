          <argument>
            <kind>1</kind>
            <string>///Controlar as configurações
if(mouse_check_button_released(mb_left))||(mouse_check_button_released(mb_right))
{
    //General
    if(point_in_rectangle(mouse_x, mouse_y, 0, 145, 320, 209))
    {
        category = "general"
    }
    if (category == "general")
    {
        if(mouse_check_button_released(mb_left))||(mouse_check_button_released(mb_right))
        {
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163, 1000, 204))
            {
                fullscreen =! fullscreen
                alarm[1] = 3;
            }
            
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163*1.5, 1000, 204*1.5))
            {
                ui_buttons =! ui_buttons
            }
            
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163*3.5, 1000, 204*3))
            {
                execute_shell(global.appdata_launcherdir, 0, 0);
            }
        }
    }
        
    //Theming
    if(point_in_rectangle(mouse_x, mouse_y, 0, 225, 320, 289))
    {
        category = "theme"
    }
    if (category == "theme")
    {
        if(mouse_check_button_released(mb_left))||(mouse_check_button_released(mb_right))
        {
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163, 1000, 204))
            {
                theme =! theme
            }
        }
        if(mouse_check_button_released(mb_left))||(mouse_check_button_released(mb_right))
        {
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163*1.5, 1000, 204*1.5))
            {
                add_wallpaper();
            }
        }
        if(mouse_check_button_released(mb_left))||(mouse_check_button_released(mb_right))
        {
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163*2, 1000, 204*2))
            {
                add_music();
            }
        }
        if(mouse_check_button_released(mb_left))||(mouse_check_button_released(mb_right))
        {
            if (point_in_rectangle(mouse_x, mouse_y, 440, 163*2.5, 1000, 204*2.5))
            {
                add_customtheme();
            }
        }
    }
    
    //Database
    if(point_in_rectangle(mouse_x, mouse_y, 0, 305, 320, 369))
    {
        category = "database"
    }
    
    //About
    if(point_in_rectangle(mouse_x, mouse_y, 0, 545, 320, 609))
    {
        category = "about"
    }
}

//Go back to the HOME Menu
if (keyboard_check_released(vk_escape))
{
    room_goto(rm_homemenu);
}
</string>
